{
  "vitest.enable": true,
  "vitest.commandLine": "npm test --",
  "cSpell.words": [
    "Akkurat",
    "bubbletip",
    "buttonstyle",
    "CBOR",
    "checkmark",
    "chpass",
    "eduid",
    "eID",
    "eidas",
    "eidplus",
    "eppn",
    "errorurl",
    "fontawesome",
    "fortawesome",
    "freja",
    "Freja",
    "Frejae",
    "immer",
    "Ladok",
    "Luhn",
    "middlewares",
    "namn",
    "newpass",
    "nin",
    "NIN",
    "Nindata",
    "NINDATA",
    "nins",
    "OIDC",
    "Ooops",
    "ORCID",
    "pdata",
    "personaldata",
    "personnummer",
    "prid",
    "pwfield",
    "recaptcha",
    "reduxjs",
    "resetpw",
    "Seleg",
    "SELEG",
    "signup",
    "Signup",
    "sitekey",
    "Skatteverket",
    "SKATTEVERKET",
    "stylelint",
    "Svenska",
    "Svenskt",
    "Svipe",
    "swamid",
    "swedenconnect",
    "tapit",
    "testid",
    "trycaptcha",
    "Typegen",
    "urldecode",
    "urlencode",
    "Userdata",
    "USERNAMEPASSWORD",
    "webauthn",
    "xstate",
    "zxcvbn"
  ],
  "cSpell.language": "en,en-US,en-GB,sv",
  "editor.rulers": [120],
  "[scss]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.stylelint": "explicit"
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "[typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  }
}
