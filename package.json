    "react-final-form": "^6.5.9",
    "react-intl": "^6.4.4",
    "react-popper": "^2.3.0",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^9.2.0",
    "react-router": "^7.5.3",
    "react-script-loader-hoc": "^1.2.2",
    "react-select": "^5.7.3",
    "react-transition-group": "^4.4.5",
    "reactstrap": "^9.2.0",
    "redux": "^5.0.1",
    "redux-logger": "^4.0.0",
    "redux-saga": "^0.16.2",
    "spin.js": "^4.1.2",
    "tslib": "^2.6.0",
    "xstate": "^5.19.2"
