    "eslint": "^8.43.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-react": "^7.32.2",
    "expect": "^29.5.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "jest-mock": "^29.0.0",
    "jquery": "^3.7.0",
    "js-cookie": "^3.0.5",
    "json-loader": "^0.5.7",
    "postcss-loader": "^7.3.3",
    "prop-types": "^15.8.1",
    "react-country-flag": "^3.1.0",
    "redux-mock-store": "^1.5.4",
