    ],
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!spin.js).+\\.js$"
