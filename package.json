{
  "name": "eduid-front",
  "version": "0.1.0",
  "description": "",
  "browser": "src/entry-points/index.js",
  "scripts": {
    "start": "rm -rf node_modules/.cache && webpack --watch --progress",
    "test": "jest",
    "build-development": "rm -rf node_modules/.cache && node ./buildPlugins.js && webpack --progress",
    "build-staging": "webpack --config webpack.staging.config.js --mode development",
    "build-production": "webpack --config webpack.prod.config.js --mode production",
    "translations:extract": "formatjs extract 'src/**/*.{js,ts,tsx}' --ignore 'src/**/*.d.ts' --out-file src/login/translation/extractedMessages.json --format transifex --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "manage:plugins:development": "node ./buildPlugins.js",
    "manage:plugins:staging": "node ./buildPlugins.staging.js",
    "manage:plugins:production": "node ./buildPlugins.prod.js",
    "serve": "webpack-dev-server --open",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "author": "Enrique PÃ©rez Arnaud",
  "license": "BSD-2-Clause",
  "engines": {
    "npm": ">=7.21.0",
    "node": ">=16.8.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@reduxjs/toolkit": "^1.6.2",
    "@ungap/url-search-params": "^0.2.2",
    "bootstrap": "^4.6.0",
    "invariant": "^2.2.4",
    "mini-create-react-context": "^0.4.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-final-form": "^6.5.7",
    "react-intl": "^5.22.0",
    "react-recaptcha": "^2.3.10",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-router-hash-link": "^2.4.3",
    "react-router-redux": "^4.0.8",
    "react-script-loader-hoc": "^1.2.2",
    "react-useragent": "^1.1.2",
    "reactstrap": "^6.5.0",
    "redux": "^4.1.2",
    "redux-form": "^8.3.8",
    "redux-logger": "^4.0.0",
    "redux-saga": "^0.16.2",
    "spin.js": "^4.1.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-flow-strip-types": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@formatjs/cli": "^4.4.0",
    "@formatjs/ts-transformer": "^3.6.0",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-router-redux": "^5.0.20",
    "@types/reactstrap": "^6.4.4",
    "@types/redux-form": "^8.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-formatjs": "10.3.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "compression-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "es6-promise": "^4.2.8",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "expect": "^27.4.2",
    "history": "^4.9.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock": "^27.4.2",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "json-loader": "^0.5.7",
    "postcss-loader": "^6.2.0",
    "precss": "^4.0.0",
    "prop-types": "^15.7.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "zxcvbn": "^4.4.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/src/tests/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "/__tests__/**/*-test.[jt]s?(x)",
      "**/?(*)+(test).[jt]s?(x)"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!spin.js|intl-messageformat|@formatjs/icu-messageformat-parser).+\\.js$"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ],
    "globals": {
      "ts-jest": {
        "astTransformers": {
          "before": [
            {
              "path": "@formatjs/ts-transformer/ts-jest-integration",
              "options": {
                "overrideIdFn": "[sha512:contenthash:base64:6]",
                "ast": true
              }
            }
          ]
        }
      }
    }
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
