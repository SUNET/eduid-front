    "ts-loader": "^9.4.4",
    "typescript": "^5.8.2",
    "webpack": "^5.88.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^5.2.1",
    "whatwg-fetch": "^3.6.2",
    "zxcvbn": "^4.4.2"
